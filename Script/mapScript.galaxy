//--------------------------------------------------------------------------------------------------
// Trigger: Ending Cinematic - Save/Exit (Normal)
//--------------------------------------------------------------------------------------------------
bool gt_EndingCinematicSaveExitNormal_Func (bool testConds, bool runActions) {
  int lv_player;
  int lv_playerIndex;


  if (!runActions) {
    return true;
  }
  UIShowCinematicText(gv_playerswatchingintrocinematic, StringToText(""), 0.05, 0.0, SoundLink("30SecondSilence", -1));
  for (PlayerGroupLoopBegin(gv_playerswatchingendingcinematic); !PlayerGroupLoopDone(); PlayerGroupLoopStep()) {
    lv_player = PlayerGroupLoopCurrent();
    lv_playerIndex = lv_player - 1;
    gv_isSavingExiting[lv_playerIndex] = true;
    gf_SaveStackCounts(lv_player);
    gf_SaveHero(lv_player);
    gf_SaveStash(lv_player);
    gf_SaveSkills(lv_player);
    gf_SaveHPC(lv_player);
    gf_SaveHRC(lv_player);
    BankValueSetFromFlag(gv_players[lv_playerIndex].banks.dragonBank, "GB", PlayerHandle(lv_player), true);
    BankSave(gv_players[lv_playerIndex].banks.dragonBank);
    if (gv_players[lv_playerIndex].type == ge_Type_FireDragon) {
      lib_talent_gf_SaveTalentBuild(1, "Fire Dragon", lv_player);
    }
    if (gv_players[lv_playerIndex].type == ge_Type_IceDragon) {
      lib_talent_gf_SaveTalentBuild(1, "Ice Dragon", lv_player);
    }
    GameOver(lv_player, c_gameOverVictory, false, true);
  }
  PlayerGroupLoopEnd();

  return true;
}
