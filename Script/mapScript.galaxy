//--------------------------------------------------------------------------------------------------
bool gt_SaveStackCountsPeriodically_Func (bool testConds, bool runActions) {
  int lv_player;
  int lv_playerIndex;


  if (testConds) {
    if (!gv_playersSetUp) {
      return false;
    }
  }

  if (!runActions) {
    return true;
  }
  for (PlayerGroupLoopBegin(gv_humanPlayers); !PlayerGroupLoopDone(); PlayerGroupLoopStep()) {
    lv_player = PlayerGroupLoopCurrent();
    lv_playerIndex = lv_player - 1;
    if (!gv_evolving[lv_playerIndex]) {
