void gf_StatCheck (int lp_player) {

  int lv_playerIndex;

  lv_playerIndex = lp_player - 1;
  if (lp_player > 4) {
    return;
  }

  if (!((UnitGetType(gv_players[lv_playerIndex].dragon) == "BabyDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "BabyIceDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "AdultDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "AdultIceDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "LegendaryDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "LegendaryIceDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "MasterDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "MasterIceDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "TeenDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "TeenIceDragon") || (UnitGetType(gv_players[lv_playerIndex].dragon) == "") || (UnitGetType(gv_players[lv_playerIndex].dragon) == null))) {
    UnitBehaviorAdd(gv_players[lv_playerIndex].dragon, "DONTEDITBANKFILES", UnitFromId(2662), 1);
  }
  gv_players[lv_playerIndex].stats.agility = UnitBehaviorCount(gv_players[lv_playerIndex].dragon, "Agility");
  gv_players[lv_playerIndex].stats.dexterity = UnitBehaviorCount(gv_players[lv_playerIndex].dragon, "Dexterity");
  gv_players[lv_playerIndex].stats.intelligence = UnitBehaviorCount(gv_players[lv_playerIndex].dragon, "Intelligence");
  gv_players[lv_playerIndex].stats.strength = UnitBehaviorCount(gv_players[lv_playerIndex].dragon, "Strength");
  gv_players[lv_playerIndex].stats.vitality = UnitBehaviorCount(gv_players[lv_playerIndex].dragon, "Vitality");
  if (
      gv_players[lv_playerIndex].stats.agility > 100 ||
      gv_players[lv_playerIndex].stats.dexterity > 500 ||
      gv_players[lv_playerIndex].stats.strength > 500 ||
      gv_players[lv_playerIndex].stats.vitality > 500 ||
      gv_players[lv_playerIndex].stats.intelligence > 500 || 
      (
        gv_players[lv_playerIndex].stats.strength +
        gv_players[lv_playerIndex].stats.dexterity +
        gv_players[lv_playerIndex].stats.intelligence +
        gv_players[lv_playerIndex].stats.vitality + 
        gv_players[lv_playerIndex].stats.agility +
        gv_players[lv_playerIndex].stats.attributePoints 
        > 
        gv_MaxLevel * 5 + 
        gv_MaxLevel / gv_LevelsPerEvolution * 5
      )
    ) {
    gv_players[lv_playerIndex].stats.attributePoints = 
      UnitGetPropertyInt(gv_players[lv_playerIndex].dragon, c_unitPropLevel, c_unitPropCurrent) * 5 +
      UnitGetPropertyInt(gv_players[lv_playerIndex].dragon, c_unitPropLevel, c_unitPropCurrent) / gv_LevelsPerEvolution * 5;
    gv_attributeText[lv_playerIndex] = IntToString(gv_players[lv_playerIndex].stats.attributePoints);
    libNtve_gf_SetDialogItemText(gv_players[lv_playerIndex].stats.attributeDialog.textBox, StringToText(gv_attributeText[lv_playerIndex]), PlayerGroupSingle(lp_player));
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "AP", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.attributePoints);
    UnitBehaviorRemovePlayer(gv_players[lv_playerIndex].dragon, "AddAgility", lp_player, gv_players[lv_playerIndex].stats.agility);
    gv_players[lv_playerIndex].stats.agility = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Ag", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.agility);
    UnitBehaviorRemovePlayer(gv_players[lv_playerIndex].dragon, "AddDexterity", lp_player, gv_players[lv_playerIndex].stats.dexterity);
    gv_players[lv_playerIndex].stats.dexterity = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "De", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.dexterity);
    UnitBehaviorRemovePlayer(gv_players[lv_playerIndex].dragon, "AddIntelligence", lp_player, gv_players[lv_playerIndex].stats.intelligence);
    gv_players[lv_playerIndex].stats.intelligence = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Int", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.intelligence);
    UnitBehaviorRemovePlayer(gv_players[lv_playerIndex].dragon, "AddStrength", lp_player, gv_players[lv_playerIndex].stats.strength);
    gv_players[lv_playerIndex].stats.strength = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Str", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.strength);
    UnitBehaviorRemovePlayer(gv_players[lv_playerIndex].dragon, "AddVitality", lp_player, gv_players[lv_playerIndex].stats.vitality);
    gv_players[lv_playerIndex].stats.vitality = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Vi", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.vitality);
    UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Agility1", true);
    UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Dexterity1", true);
    UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Intelligence1", true);
    UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Strength1", true);
    UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Vitality1", true);
    UIDisplayMessage(PlayerGroupSingle(lp_player), c_messageAreaError, StringExternal("Param/Value/21DD33BF"));
  }
}
trigger auto_gf_Save_Trigger = null;
int auto_gf_Save_lp_player;
void gf_Save (int lp_player) {
  auto_gf_Save_lp_player = lp_player;
  if (auto_gf_Save_Trigger == null) {
    auto_gf_Save_Trigger = TriggerCreate("auto_gf_Save_TriggerFunc");
  }
  TriggerExecute(auto_gf_Save_Trigger, false, false);
}
bool auto_gf_Save_TriggerFunc (bool testConds, bool runActions) {
  int lp_player = auto_gf_Save_lp_player;

  int lv_playerIndex;

  lv_playerIndex = lp_player - 1;

  if (!gv_isSavingExiting[lv_playerIndex] && !gv_autosavePrevention[lv_playerIndex] && gv_playersSetUp) {
    gv_isSavingExiting[lv_playerIndex] = true;
    gf_SaveStackCounts(lp_player);
    gf_SaveHero(lp_player);
    gf_SaveStash(lp_player);
    gf_SaveSkills(lp_player);
    gf_SaveHPC(lp_player);
    gf_SaveHRC(lp_player);
    BankValueSetFromFlag(gv_players[lv_playerIndex].banks.dragonBank, "GB", PlayerHandle(lp_player), gv_gameBeat[lv_playerIndex]);
    BankSave(gv_players[lv_playerIndex].banks.dragonBank);
    if (gv_players[lv_playerIndex].type == ge_Type_FireDragon) {
      lib_talent_gf_SaveTalentBuild(1, "Fire Dragon", lp_player);
    }
    if (gv_players[lv_playerIndex].type == ge_Type_IceDragon) {
      lib_talent_gf_SaveTalentBuild(1, "Ice Dragon", lp_player);
    }
    gv_isSavingExiting[lv_playerIndex] = false;
  }
  return true;
}
void gf_SaveHero (int lp_player) {
  int lv_playerIndex;
  lv_playerIndex = lp_player - 1;

  if ((PlayerStatus(lp_player) == c_playerStatusActive) && gv_playersSetUp && UnitIsAlive(gv_players[lv_playerIndex].dragon) && gv_players[lv_playerIndex].verification.dragon) {
    BankValueSetFromUnit(gv_players[lv_playerIndex].banks.dragonBank, "Dragon", PlayerHandle(lp_player), gv_players[lv_playerIndex].dragon);
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "DSN", PlayerHandle(lp_player), gv_DSNMod + UnitLevel(gv_players[lv_playerIndex].dragon) + ModI(FixedToInt(UnitGetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropXP, c_unitPropCurrent)), 1000));
  }
}
