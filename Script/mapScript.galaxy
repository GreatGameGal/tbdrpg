void gf_LoadSkills (int lp_player) {
  auto_gf_LoadSkills_lp_player = lp_player;
  if (auto_gf_LoadSkills_Trigger == null) {
    auto_gf_LoadSkills_Trigger = TriggerCreate("auto_gf_LoadSkills_TriggerFunc");
  }
  TriggerExecute(auto_gf_LoadSkills_Trigger, false, false);
}
bool auto_gf_LoadSkills_TriggerFunc (bool testConds, bool runActions) {
  int lv_player = auto_gf_LoadSkills_lp_player;
  int lv_playerIndex;
  int lv_i;

  lv_playerIndex = lv_player - 1;

  if (BankSectionExists(gv_players[lv_playerIndex].banks.dragonBank, "FDFB")) {
    gv_players[lv_playerIndex].skills.fireball = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "FDFB", PlayerHandle(lv_player));
    if ((gv_players[lv_playerIndex].skills.fireball >= 0)) {
      if (!UnitAbilityCheck(gv_players[lv_playerIndex].dragon, "Fireball", true)) {
        for (lv_i = 0; lv_i <= gv_players[lv_playerIndex].skills.fireball; lv_i += 1) {
          UnitIssueOrder(gv_players[lv_playerIndex].dragon, Order(AbilityCommand("LearnSkillsFire", 0)), c_orderQueueAddToEnd);
        }
      }
      else {
        for (lv_i = UnitAbilityGetLevel(gv_players[lv_playerIndex].dragon, "Fireball") + 1; lv_i <= gv_players[lv_playerIndex].skills.fireball; lv_i += 1 ) {
          UnitIssueOrder(gv_players[lv_playerIndex].dragon, Order(AbilityCommand("LearnSkillsFire", 0)), c_orderQueueAddToEnd);
        }
      }
    }
  }
  if (BankSectionExists(gv_players[lv_playerIndex].banks.dragonBank, "FDFC")) {
    gv_players[lv_playerIndex].skills.fireClaw = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "FDFC", PlayerHandle(lv_player));
    if ((gv_players[lv_playerIndex].skills.fireClaw >= 0)) {
      if (!UnitAbilityCheck(gv_players[lv_playerIndex].dragon, "FireClaws", true)) {
        for (lv_i = 0; lv_i <= gv_players[lv_playerIndex].skills.fireClaw; lv_i += 1) {
          UnitIssueOrder(gv_players[lv_playerIndex].dragon, Order(AbilityCommand("LearnSkillsFire", 1)), c_orderQueueAddToEnd);
        }
      }
      else {
        for (lv_i = UnitAbilityGetLevel(gv_players[lv_playerIndex].dragon, "FireClaws") + 1; lv_i <= gv_players[lv_playerIndex].skills.fireClaw; lv_i += 1 ) {
          UnitIssueOrder(gv_players[lv_playerIndex].dragon, Order(AbilityCommand("LearnSkillsFire", 1)), c_orderQueueAddToEnd);
        }
      }
    }
  }
