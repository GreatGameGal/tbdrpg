bool gt_LoadShards_Func (bool testConds, bool runActions) {
  int lv_player;
  int lv_playerIndex;

  if (!runActions) {
    return true;
  }
  for(lv_playerIndex = 0; lv_playerIndex < 4; lv_playerIndex += 1) {
    if ((PlayerStatus(lv_player) == c_playerStatusActive)) {
      lv_player = lv_player + 1;
      while (gv_players[lv_playerIndex].banks.accessing) {
        Wait(1.0, c_timeGame);
      }
      gv_players[lv_playerIndex].banks.accessing = true;
      gv_shards[lv_playerIndex] = (BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Shards", PlayerHandle(lv_player)) / 1);
      PlayerModifyPropertyInt(lv_player, c_playerPropCustom, c_playerPropOperSetTo, gv_shards[lv_playerIndex]);
      gv_players[lv_playerIndex].banks.accessing = false;
    }
  }
  return true;
}
