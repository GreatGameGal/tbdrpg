      gv_players[lv_playerIndex].quests.manaQuests[3].kills += 1;
      ObjectiveSetName(gv_players[lv_playerIndex].quests.manaQuests[3].objective, (StringExternal("Param/Value/8FDF552C") + (IntToText((10 - gv_players[lv_playerIndex].quests.manaQuests[3].kills)) + StringExternal("Param/Value/16FD1AF9"))));
