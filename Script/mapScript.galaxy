bool lib_talent_gt_CloseTalent_Func (bool testConds, bool runActions) {
  int lv_player;
  int lv_playerIndex;

  lv_player = EventPlayer();
  lv_playerIndex = lv_player - 1;

  if (testConds) {
    if (!((EventDialogControl() == gv_players[lv_playerIndex].talent.closeTalentTreeButton))) {
      return false;
    }
  }

  if (!runActions) {
    return true;
  }
  lib_talent_gf_ShowHideTalentTree(lv_player);
  return true;
}
