  int lv_player;
  int lv_playerIndex;


  if (!runActions) {
    return true;
  }
  for (PlayerGroupLoopBegin(gv_playersInBioDome); !PlayerGroupLoopDone(); PlayerGroupLoopStep()) {
    lv_player = PlayerGroupLoopCurrent();
    lv_playerIndex = lv_player - 1;
    if ((gv_players[lv_playerIndex].quests.storyQuests[8].state == 7)) {
      gv_players[lv_playerIndex].quests.storyQuests[8].state = 8;
      BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "SQ8", PlayerHandle(lv_player), gv_players[lv_playerIndex].quests.storyQuests[8].state);
      ObjectiveSetState(gv_players[lv_playerIndex].quests.storyQuests[8].objective, c_objectiveStateCompleted);
      ObjectiveDestroy(gv_players[lv_playerIndex].quests.storyQuests[8].objective);
      if ((UnitLevel(gv_players[lv_playerIndex].dragon) != 100)) {
        gf_AddExperience(gv_players[lv_playerIndex].dragon, 25000.0);
      }
      UIDisplayMessage(gv_players[lv_player].playerGroupSingle, c_messageAreaSubtitle, StringExternal("Param/Value/2725B5FA"));
      PingDestroy(gv_players[lv_playerIndex].quests.storyQuests[8].ping);
      gf_SQ9Setup(lv_player);
    }
  }
  PlayerGroupLoopEnd();

  return true;
}
//--------------------------------------------------------------------------------------------------
void gt_SQ8Crystal6Destroyed_Init () {
  gt_SQ8Crystal6Destroyed = TriggerCreate("gt_SQ8Crystal6Destroyed_Func");
  TriggerAddEventUnitDied(gt_SQ8Crystal6Destroyed, UnitRefFromUnit(UnitFromId(2501)));
}
