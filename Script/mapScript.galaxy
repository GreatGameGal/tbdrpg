//--------------------------------------------------------------------------------------------------
// Trigger: Syrital Dies
//--------------------------------------------------------------------------------------------------
bool gt_SyritalDies_Func (bool testConds, bool runActions) {
  
  int lv_player;
  int lv_playerIndex;


  if (!runActions) {
    return true;
  }

  SoundStop(gv_syritalsTheme[0], true);
  SoundStop(gv_syritalsTheme[1], true);
  SoundStop(gv_syritalsTheme[2], true);
  SoundStop(gv_syritalsTheme[3], true);
  for (PlayerGroupLoopBegin(gv_humanPlayers); !PlayerGroupLoopDone(); PlayerGroupLoopStep() ) {
    lv_player = PlayerGroupLoopCurrent();
    lv_playerIndex = lv_player - 1;

    if (gv_inFightwithSyrital[lv_playerIndex]) {
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategoryCombat, 100.0, 1.0);
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategoryVoice, 100.0, 1.0);
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategorySpell, 100.0, 1.0);
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategoryAmbient, 100.0, 1.0);
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategoryAmbient, 100.0, 1.0);
      SoundChannelSetVolume(gv_players[lv_playerIndex].playerGroupSingle, c_soundCategoryUI, 100.0, 1.0);
      gv_inFightwithSyrital[lv_playerIndex] = false;
      libNtve_gf_ShowHideBossBar(false, lv_player);
    }
  }
  PlayerGroupLoopEnd();

  UIDisplayMessage(PlayerGroupAll(), c_messageAreaSubtitle, StringExternal("Param/Value/D94B490A"));
  gv_syritalDead = true;
  TriggerEnable(gt_SyritalRespawn, true);
  UnitGroupLoopBegin(gv_unitsthatdidDamagetoSyrital);
  for ( ; !UnitGroupLoopDone(); UnitGroupLoopStep() ) {
    lv_player = UnitGetOwner(UnitGroupLoopCurrent());
    lv_playerIndex = lv_player - 1;
    gf_AddExperience(UnitGroupLoopCurrent(), 500.0);
    PlayerModifyPropertyInt(lv_player, c_playerPropCustom, c_playerPropOperAdd, 1);
    if ((gv_players[lv_playerIndex].quests.storyQuests[1].state == 1)) {
      gv_players[lv_playerIndex].quests.storyQuests[1].state = 2;
      ObjectiveSetDescription(gv_players[lv_playerIndex].quests.storyQuests[1].objective, StringExternal("Param/Value/31DF3212"));
      ObjectiveSetName(gv_players[lv_playerIndex].quests.storyQuests[1].objective, StringExternal("Param/Value/05FD217C"));
      SoundPlay(SoundLink("Alert_UniObjectiveUpdate", -1), gv_players[lv_playerIndex].playerGroupSingle, 100.0, 0.0);
      BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "SQ1", PlayerHandle(UnitGetOwner(UnitGroupLoopCurrent())), gv_players[lv_playerIndex].quests.storyQuests[1].state);
      TextTagCreate(StringExternal("Param/Value/3A58ACB6"), 72, UnitGetPosition(UnitFromId(2511)), 6.0, true, true, gv_players[lv_playerIndex].playerGroupSingle);
      gv_players[lv_playerIndex].quests.storyQuests[1].textTag = TextTagLastCreated();
      TextTagSetColor(TextTagLastCreated(), c_textTagColorText, Color(100.00, 100.00, 0.00));
      TextTagSetColor(TextTagLastCreated(), c_textTagColorEdge, Color(100.00, 100.00, 100.00));
      TextTagAttachToUnit(TextTagLastCreated(), UnitFromId(2511), 5.0);
      TextTagSetTime(TextTagLastCreated(), c_textTagTimeDuration, 524287.0);
      PingDestroy(gv_players[lv_playerIndex].quests.storyQuests[1].ping);
      PingCreate(gv_players[lv_playerIndex].playerGroupSingle, "PingObjective", RegionGetCenter(RegionFromId(216)), ColorWithAlpha(0.00, 100.00, 0.00, 0.00), 524287.0);
      gv_players[lv_playerIndex].quests.storyQuests[1].ping = PingLastCreated();
      PingSetScale(PingLastCreated(), 0.5);
    }
  }
  UnitGroupLoopEnd();
  return true;
}
