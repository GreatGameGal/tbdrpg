  lv_playerIndex = lp_player - 1;

  if ((PlayerStatus(lp_player) == c_playerStatusActive)) {
    if (BankSectionExists(gv_players[lv_playerIndex].banks.currencyBank, "HPC")) {
      gv_shards[lv_playerIndex] = (BankValueGetAsInt(gv_players[lv_playerIndex].banks.currencyBank, "Shards", PlayerHandle(lp_player)) / 1);
      PlayerModifyPropertyInt(lp_player, c_playerPropCustom, c_playerPropOperSetTo, gv_shards[lv_playerIndex]);
      gv_gold[lv_playerIndex] = (BankValueGetAsInt(gv_players[lv_playerIndex].banks.currencyBank, "Gold", PlayerHandle(lp_player)) / 1);
      PlayerModifyPropertyInt(lp_player, c_playerPropMinerals, c_playerPropOperSetTo, gv_gold[lv_playerIndex]);
      gv_pvPPoints[lv_playerIndex] = (BankValueGetAsInt(gv_players[lv_playerIndex].banks.currencyBank, "PvP Points", PlayerHandle(lp_player)) / 1);
      PlayerModifyPropertyInt(lp_player, c_playerPropVespene, c_playerPropOperSetTo, gv_pvPPoints[lv_playerIndex]);
      lv_hPCManual = (gv_shards[lv_playerIndex] + (gv_gold[lv_playerIndex] + (gv_pvPPoints[lv_playerIndex] + gv_HPCMod)));
      lv_hPCBank = BankValueGetAsInt(gv_players[lv_playerIndex].banks.currencyBank, "HPC", PlayerHandle(lp_player));
      if ((lv_hPCBank != lv_hPCManual)) {
        if ((lv_hPCBank == -10691)) {
          UIDisplayMessage(gv_players[lv_playerIndex].playerGroupSingle, c_messageAreaSubtitle, StringExternal("Param/Value/10F95778"));
          gv_players[lv_playerIndex].verification.hpc = true;
        }
        else {
          UIDisplayMessage(gv_players[lv_playerIndex].playerGroupSingle, c_messageAreaSubtitle, StringExternal("Param/Value/7CC796A6"));
          GameOver(lp_player, c_gameOverDefeat, true, true);
        }
      }
      else {
        gv_players[lv_playerIndex].verification.hpc = true;
      }
    }
    else {
      if (BankSectionExists(gv_players[lv_playerIndex].banks.dragonBank, "Gold") && BankSectionExists(gv_players[lv_playerIndex].banks.dragonBank, "PvP Points") && BankSectionExists(gv_players[lv_playerIndex].banks.dragonBank, "Shards")) {
        UIDisplayMessage(gv_players[lv_playerIndex].playerGroupSingle, c_messageAreaSubtitle, StringExternal("Param/Value/A7264C2D"));
        GameOver(lp_player, c_gameOverDefeat, true, true);
      }
      else {
        gv_players[lv_playerIndex].verification.hpc = true;
      }
    }
  }
