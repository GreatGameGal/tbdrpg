      // Evolve to the next evolution of the type.
      gf_EvolveTo(lv_player, gv_DragonUnitNames[gv_players[lv_playerIndex].type][lv_dragonEvolution+1]);
    }
  }
  PlayerGroupLoopEnd();
  return true;
}
