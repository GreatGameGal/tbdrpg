void gf_InitializePlayers () {
  int lv_player;
  int lv_playerIndex;
  int lv_otherPlayer;
  // Loops through all players to initialize each one.
  for (lv_player=1;lv_player<=4;lv_player+=1) {
    if ((PlayerStatus(lv_player) == c_playerStatusActive) && (PlayerType(lv_player) == c_playerTypeUser)) {
      lv_playerIndex = lv_player - 1;
      gv_players[lv_playerIndex].playerGroupSingle = PlayerGroupSingle(lv_player);

      if (GameAttributePlayerValue("1", lv_player) == "0001") {
        gv_players[lv_playerIndex].type = ge_Type_FireDragon;
      } else if (GameAttributePlayerValue("1", lv_player) == "0002") {
        gv_players[lv_playerIndex].type = ge_Type_IceDragon;
      }

      // Temporary until I handle setup of player.
      gf_SetupPlayer(lv_player);
      libNtve_gf_PauseUnit(gv_players[lv_playerIndex].dragon, true);
      MeleeSetOption(lv_player, c_meleeOptionSplitResources, false);
      PlayerSetState(lv_player, c_playerStateDisplayGameResult, false);
      // Loop through all players to set ally values for other players.
      for (lv_otherPlayer = 1;lv_otherPlayer<=4;lv_otherPlayer+=1) {
        // Only set ally value if player isn't other.
        if (lv_player != lv_otherPlayer) {
          PlayerSetAlliance(lv_player, c_allianceIdTrade, lv_otherPlayer, false);
        }
      }
      libNtve_gf_SetAlliance(lv_player, 15, libNtve_ge_AllianceSetting_Enemy);
      gf_StatCheck(lv_player);
      gf_LoadStackCounts(lv_player);
      gf_ReapplyBuffs(lv_player);
      gf_ChangeItemsOwner(lv_player);
      gf_LoadSkills(lv_player);
      gf_LoadStash(lv_player);
      UnitSelect(gv_players[lv_playerIndex].dragon, lv_player, true);
      gv_players[lv_playerIndex].stats.arena.wins = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Wins", PlayerHandle(lv_player));
      gv_players[lv_playerIndex].stats.arena.losses = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Losses", PlayerHandle(lv_player));
    }
  }
}
