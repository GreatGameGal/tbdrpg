void gf_OcculusPingPlayer(int lp_player) {
  int lv_playerIndex = lp_player-1;
  UnitGroupLoopBegin(UnitGroupAlliance(lp_player, c_unitAllianceEnemy, RegionCircle(UnitGetPosition(gv_players[lv_playerIndex].dragon), 50.0), UnitFilter(0, 0, (1 << c_targetFilterMissile) | (1 << c_targetFilterItem), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 1000));
  for ( ; !UnitGroupLoopDone(); UnitGroupLoopStep() ) {
    libNtve_gf_CreatePingFacingAngle(gv_players[lv_playerIndex].playerGroupSingle, "PingNumeric0", UnitGetPosition(UnitGroupLoopCurrent()), ColorWithAlpha(100.00, 0.00, 0.00, 0.00), 6.0, 270.0);
    PingSetScale(PingLastCreated(), 0.2);
    PingSetUnit(PingLastCreated(), UnitGroupLoopCurrent());
  }
  UnitGroupLoopEnd();
}
