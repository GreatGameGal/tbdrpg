//--------------------------------------------------------------------------------------------------
bool gt_PlayerLeaves_Func (bool testConds, bool runActions) {
  int lv_player;
  int lv_playerIndex;

  lv_player = EventPlayer();
  lv_playerIndex = lv_player - 1;
  if (testConds) {
    if (!PlayerGroupHasPlayer(gv_humanPlayers, lv_player)) {
      return false;
    }
  }

  if (!runActions) {
    return true;
  }
  PlayerGroupRemove(gv_humanPlayers, lv_player);
  UnitRemove(gv_players[lv_playerIndex].dragon);
  PlayerSetAlliance(lv_player, c_allianceIdSpend, 1, false);
  PlayerSetAlliance(lv_player, c_allianceIdSpend, 2, false);
  PlayerSetAlliance(lv_player, c_allianceIdSpend, 3, false);
  PlayerSetAlliance(lv_player, c_allianceIdSpend, 4, false);
  PlayerSetAlliance(1, c_allianceIdSpend, lv_player, false);
  PlayerSetAlliance(2, c_allianceIdSpend, lv_player, false);
  PlayerSetAlliance(3, c_allianceIdSpend, lv_player, false);
  PlayerSetAlliance(4, c_allianceIdSpend, lv_player, false);
  libNtve_gf_SetAlliance(lv_player, 1, libNtve_ge_AllianceSetting_Enemy);
  libNtve_gf_SetAlliance(lv_player, 2, libNtve_ge_AllianceSetting_Enemy);
  libNtve_gf_SetAlliance(lv_player, 3, libNtve_ge_AllianceSetting_Enemy);
  libNtve_gf_SetAlliance(lv_player, 4, libNtve_ge_AllianceSetting_Enemy);
  PlayerModifyPropertyInt(lv_player, c_playerPropMinerals, c_playerPropOperSetTo, 0);
  PlayerModifyPropertyInt(lv_player, c_playerPropVespene, c_playerPropOperSetTo, 0);
  PlayerModifyPropertyInt(lv_player, c_playerPropTerrazine, c_playerPropOperSetTo, 0);
