void gf_SetupPlayer(int lp_player) {
  int lv_tempAgility;
  int lv_tempDexterity;
  int lv_tempIntelligence;
  int lv_tempStrength;
  int lv_tempVitality;
  int lv_tempReborn;
  int lv_type;
  int lv_playerIndex = lp_player-1;
  if ((GameAttributePlayerValue("1", lp_player) == "0001")) {
    lv_type = 0;
  } else if (GameAttributePlayerValue("1", lp_player) == "0002") {
    lv_type = 1;
  }
  // Load banks
  BankLoad("DragonRPGStashBank", lp_player);
  gv_players[lv_playerIndex].banks.stashBank = BankLastCreated();
  BankWait(gv_players[lv_playerIndex].banks.stashBank);
  BankLoad("DragonRPGCurrencyBank", lp_player);
  gv_players[lv_playerIndex].banks.currencyBank = BankLastCreated();
  BankWait(gv_players[lv_playerIndex].banks.currencyBank);
  BankLoad("DragonRPGRebornBank", lp_player);
  gv_players[lv_playerIndex].banks.rebornBank = BankLastCreated();
  BankWait(gv_players[lv_playerIndex].banks.rebornBank);
  CameraPan(lp_player, PointFromId(1), 0.0, -1, 0, false);
  gv_ofHumanPlayers = (gv_ofHumanPlayers + 1);
  if (lv_type == ge_Type_FireDragon) {
    BankLoad("DragonRPGFireBank", lp_player);
  } else if (lv_type == ge_Type_IceDragon) {
    BankLoad("DragonRPGIceBank", lp_player);
  }
  gv_players[lv_playerIndex].banks.dragonBank = BankLastCreated();
  BankWait(gv_players[lv_playerIndex].banks.dragonBank);
  gv_players[lv_playerIndex].verification.dragon = false;
  if (BankKeyExists(gv_players[lv_playerIndex].banks.dragonBank, "Dragon", PlayerHandle(lp_player))) {
    TipAlertPanelClear(gv_players[lv_playerIndex].playerGroupSingle);
    TechTreeRequirementsEnable(lp_player, false);
    BankValueGetAsUnit(gv_players[lv_playerIndex].banks.dragonBank, "Dragon", PlayerHandle(lp_player), lp_player, PointFromId(1), 0.0);
    UnitSelect(BankLastRestoredUnit(), lp_player, true);
    gv_players[lv_playerIndex].dragon = BankLastRestoredUnit();
    gv_players[lv_playerIndex].stats.level = FixedToInt(UnitGetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropLevel, c_unitPropCurrent));
    gv_players[lv_playerIndex].stats.reborn.count = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Reborn Count", PlayerHandle(lp_player));
    lv_tempReborn = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Reborn Count", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].stats.attributePoints = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "AP", PlayerHandle(lp_player));
    // TODO: Remove this after a few updates
    if (gv_players[lv_playerIndex].stats.attributePoints == 0) {
      gv_players[lv_playerIndex].stats.attributePoints = FixedToInt(BankValueGetAsFixed(gv_players[lv_playerIndex].banks.dragonBank, "AP", PlayerHandle(lp_player)));
    }
    lv_tempVitality = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Vi", PlayerHandle(lp_player));
    lv_tempStrength = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Str", PlayerHandle(lp_player));
    lv_tempIntelligence = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Int", PlayerHandle(lp_player));
    lv_tempDexterity = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "De", PlayerHandle(lp_player));
    lv_tempAgility = BankValueGetAsInt(gv_players[lv_playerIndex].banks.dragonBank, "Ag", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.fireSkin = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "FS", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.allSeeingEye = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "ASE", PlayerHandle(lp_player));
    gv_gameBeat[lv_playerIndex] = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "GB", PlayerHandle(lp_player));
    gv_playerhasseenIntro[lv_playerIndex] = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "Intro", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.warningOcculus = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "OOW", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.revengeFacet = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "FOR", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.rebirthCrystal = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "COR", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.returnJewel = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "JOR", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.protectionStone = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "SOR", PlayerHandle(lp_player));
    gv_players[lv_playerIndex].skills.quickeningRing = BankValueGetAsFlag(gv_players[lv_playerIndex].banks.dragonBank, "ROQ", PlayerHandle(lp_player));
    if (gv_players[lv_playerIndex].skills.warningOcculus) {
      if (lp_player == 1) {
        TriggerEnable(gt_OcculusPingPlayer1, true);
      } else if (lp_player == 2) {
        TriggerEnable(gt_OcculusPingPlayer2, true);
      } else if (lp_player == 3) {
        TriggerEnable(gt_OcculusPingPlayer3, true);
      } else if (lp_player == 4) {
        TriggerEnable(gt_OcculusPingPlayer4, true);
      }
    }
    if (gv_players[lv_playerIndex].skills.fireSkin) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "FireSkin", lp_player, 1);
    }
    if (gv_players[lv_playerIndex].skills.allSeeingEye) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AllSeeingEye", lp_player, 1);
    }
    while ((lv_tempVitality > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AddVitality", lp_player, 1);
      gv_players[lv_playerIndex].stats.vitality = (gv_players[lv_playerIndex].stats.vitality + 1);
      lv_tempVitality = (lv_tempVitality - 1);
    }
    while ((lv_tempStrength > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AddStrength", lp_player, 1);
      gv_players[lv_playerIndex].stats.strength = (gv_players[lv_playerIndex].stats.strength + 1);
      lv_tempStrength = (lv_tempStrength - 1);
    }
    while ((lv_tempIntelligence > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AddIntelligence", lp_player, 1);
      gv_players[lv_playerIndex].stats.intelligence = (gv_players[lv_playerIndex].stats.intelligence + 1);
      lv_tempIntelligence = (lv_tempIntelligence - 1);
    }
    while ((lv_tempDexterity > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AddDexterity", lp_player, 1);
      gv_players[lv_playerIndex].stats.dexterity = (gv_players[lv_playerIndex].stats.dexterity + 1);
      lv_tempDexterity = (lv_tempDexterity - 1);
    }
    while ((lv_tempAgility > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "AddAgility", lp_player, 1);
      gv_players[lv_playerIndex].stats.agility = (gv_players[lv_playerIndex].stats.agility + 1);
      lv_tempAgility = (lv_tempAgility - 1);
    }
    while ((lv_tempReborn > 0)) {
      UnitBehaviorAddPlayer(gv_players[lv_playerIndex].dragon, "BlessingofDrache", lp_player, 1);
      lv_tempReborn = (lv_tempReborn - 1);
    }
    if ((gv_players[lv_playerIndex].stats.attributePoints == 0)) {
      UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Agility1", false);
      UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Dexterity1", false);
      UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Intelligence1", false);
      UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Strength1", false);
      UnitAbilityShow(gv_players[lv_playerIndex].dragon, "Vitality1", false);
    }
    else {
      gv_attributeText[lv_playerIndex] = IntToString(gv_players[lv_playerIndex].stats.attributePoints);
      libNtve_gf_SetDialogItemText(gv_players[lv_playerIndex].stats.attributeDialog.textBox, StringToText(gv_attributeText[lv_playerIndex]), gv_players[lv_playerIndex].playerGroupSingle);
    }
    UnitSetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropLifePercent, 100.0);
    UnitSetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropEnergyPercent, 100.0);
    UnitSetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropShieldsPercent, 100.0);
    BankValueSetFromUnit(gv_players[lv_playerIndex].banks.dragonBank, "Dragon", PlayerHandle(lp_player), gv_players[lv_playerIndex].dragon);
    BankSectionRemove(gv_players[lv_playerIndex].banks.dragonBank, "Minerals");
    BankSectionRemove(gv_players[lv_playerIndex].banks.dragonBank, "Minerals2");
    BankSectionRemove(gv_players[lv_playerIndex].banks.dragonBank, "Vespene");
    TechTreeRequirementsEnable(lp_player, true);
  } else {
    if (lv_type == ge_Type_FireDragon) {
      UnitCreate(1, "BabyDragon", 0, lp_player, PointFromId(1), 270.0);
    } else if (lv_type == ge_Type_IceDragon) {
      UnitCreate(1, "BabyIceDragon", 0, lp_player, PointFromId(1), 270.0);
    }
    UnitSelect(UnitLastCreated(), lp_player, true);
    gv_players[lv_playerIndex].dragon = UnitLastCreated();
    BankValueSetFromUnit(gv_players[lv_playerIndex].banks.dragonBank, "Dragon", PlayerHandle(lp_player), gv_players[lv_playerIndex].dragon);
    gv_players[lv_playerIndex].stats.level = FixedToInt(UnitGetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropLevel, c_unitPropCurrent));
    gv_players[lv_playerIndex].stats.agility = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Ag", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.agility);
    gv_players[lv_playerIndex].stats.dexterity = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "De", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.dexterity);
    gv_players[lv_playerIndex].stats.intelligence = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Int", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.intelligence);
    gv_players[lv_playerIndex].stats.strength = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Str", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.strength);
    gv_players[lv_playerIndex].stats.vitality = 0;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "Vi", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.vitality);
    gv_players[lv_playerIndex].stats.attributePoints = 5;
    BankValueSetFromInt(gv_players[lv_playerIndex].banks.dragonBank, "AP", PlayerHandle(lp_player), gv_players[lv_playerIndex].stats.attributePoints);
    // DSN
    gv_players[lv_playerIndex].verification.dragon = true;
    UnitInventoryCreate(gv_players[lv_playerIndex].dragon, "BasicClaws");
    UnitInventoryCreate(gv_players[lv_playerIndex].dragon, "BasicSpines");
  }
  PlayerGroupAdd(gv_humanPlayers, lp_player);
  UnitGroupAdd(gv_dragons, gv_players[lv_playerIndex].dragon);
  UnitControlGroupAddUnit(lp_player, 1, gv_players[lv_playerIndex].dragon);
  if ((UnitGetPropertyFixed(gv_players[lv_playerIndex].dragon, c_unitPropLevel, c_unitPropCurrent) == 100.0) && gv_gameBeat[lv_playerIndex] ) {
    UnitXPGainEnable(gv_players[lv_playerIndex].dragon, null, false);
    HelpPanelAddTip(gv_players[lv_playerIndex].playerGroupSingle, libNtve_gf_FormatTipTitle(StringExternal("Param/Value/95517AFD"), libNtve_ge_TipType_NormalTip), StringExternal("Param/Value/BC7D2081"), StringExternal("Param/Value/FC8D3D85"), "Dragon Songs\\btn-ability-zerg-consume-red.dds");
  }
}
