void gf_WaypointTeleport (int lp_player, region lp_waypoint) {

  int lv_playerIndex;
  actor lv_effectActor1;
  actor lv_effectActor2;

  lv_playerIndex = lp_player - 1;

  libNtve_gf_PauseUnit(gv_players[lv_playerIndex].dragon, true);
  libNtve_gf_CreateModelAtPoint("TimeWarpLaunch", UnitGetPosition(gv_players[lv_playerIndex].dragon));
  lv_effectActor1 = libNtve_gf_ActorLastCreated();
  SoundPlayOnUnit(SoundLink("WarpGate_TrainStart", -1), PlayerGroupAll(), gv_players[lv_playerIndex].dragon, 0.0, 100.0, 0.0);
  Wait(0.45, c_timeGame);
  UnitSetPosition(gv_players[lv_playerIndex].dragon, RegionGetCenter(lp_waypoint), false);
  gv_respawnPoint[lv_playerIndex] = RegionGetCenter(lp_waypoint);
  libNtve_gf_CreateModelAtPoint("TimeWarp", RegionGetCenter(lp_waypoint));
  lv_effectActor2 = libNtve_gf_ActorLastCreated();
  SoundPlayOnUnit(SoundLink("WarpGate_TrainFinish", -1), PlayerGroupAll(), gv_players[lv_playerIndex].dragon, 0.0, 100.0, 0.0);
  Wait(0.45, c_timeGame);
  libNtve_gf_KillModel(lv_effectActor1);
  CameraPan(lp_player, RegionGetCenter(lp_waypoint), 0.0, -1, 10.0, false);
  libNtve_gf_PauseUnit(gv_players[lv_playerIndex].dragon, false);
  Wait(0.45, c_timeGame);
  libNtve_gf_KillModel(lv_effectActor2);
}
